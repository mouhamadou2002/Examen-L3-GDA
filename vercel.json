{
    "buildCommand": "npm run build",
    "outputDirectory": "public/build",
    "framework": "laravel",
    "functions": {
        "public/index.php": {
            "runtime": "vercel-php@0.6.0"
        }
    },
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/public/index.php"
        }
    ]
} 